{
  "name": "@aksara-ui/icons",
  "version": "1.0.0-canary.10",
  "description": "React icon library for the Aksara design system.",
  "sideEffects": false,
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "exports": "./dist/index.modern.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "aksara:src": "./src/index.ts",
  "repository": "https://github.com/kata-ai/wicara.git",
  "author": "Kata.ai <dev@kata.ai>",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "microbundle --no-compress --format modern,es,cjs --jsx React.createElement",
    "clean": "rimraf dist",
    "dev": "microbundle watch --jsx React.createElement",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepublish": "yarn build",
    "start": "microbundle watch --jsx React.createElement",
    "validate": "yarn lint && yarn type-check"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  },
  "peerDependencies": {
    "react": "^16.10.0",
    "react-dom": "^16.10.0"
  },
  "devDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
